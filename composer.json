{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-fileinfo": "*",
        "ext-gettext": "*",
        "ext-json": "*",
        "artem-schander/l5-modular": "^1.4",
        "barryvdh/laravel-dompdf": "^0.8.4",
        "barryvdh/laravel-snappy": "^0.4.4",
        "brian2694/laravel-toastr": "^5.5",
        "daltcore/lara-pdf-merger": "^2.0",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "fideloper/proxy": "~4.0",
        "genealabs/laravel-model-caching": "0.7.*",
        "grofgraf/laravel-pdf-merger": "^1.0",
        "guzzlehttp/guzzle": "^6.4",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "maatwebsite/excel": "^3.1",
        "maddhatter/laravel-fullcalendar": "^1.3",
        "mcamara/laravel-localization": "^1.3",
        "renatomarinho/laravel-page-speed": "^1.8",
        "spatie/laravel-backup": "^6.2",
        "spatie/laravel-permission": "^2.31",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "timgws/query-builder-parser": "^1.5",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.5",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/telescope": "^2.1",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\GA\\": "app/Modules/GA/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
